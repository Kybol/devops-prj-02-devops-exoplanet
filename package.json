{
    "name": "exojavascriptplanet",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "node app.js",
        "commit": "cz",
        "prettier": "prettier -c \"**/*.js\"",
        "prettier:fix": "prettier -w \"**/*.js\"",
        "lint": "eslint \"**/*.js\"",
        "lint:fix": "eslint --fix \"**/*.js\"",
        "code:fix": "npm run prettier:fix && npm run lint:fix",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "test:watch": "jest --watchAll"
    },
    "dependencies": {
        "cookie-parser": "~1.4.4",
        "debug": "~2.6.9",
        "express": "~4.16.1",
        "hbs": "~4.0.4",
        "http-errors": "~1.6.3",
        "morgan": "~1.9.1"
    },
    "devDependencies": {
        "commitizen": "^4.2.6",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.30.0",
        "eslint-config-prettier": "8.5.0",
        "jest": "^29.3.1",
        "nodemon": "^2.0.15",
        "prettier": "2.8.1"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
